{
  "name": "Medula",
  "version": "0.4.0",
  "dependencies": {},
  "devDependencies": {
    "normalize.css": "*",
    "susy": "*",
    "include-media": "*",
    "sassy-maps": "*",
    "modernizr": "*",
    "jQuery.mmenu": "*",
    "nwmatcher": "*",
    "respond": "*",
    "selectivizr": "*",
    "animate-sass": "*"
  },
  "ignore": [ "" ],
  "preen": {
    "normalize.css": [
      "*.css"
    ],
    "susy": [
      "sass/**/*.scss"
    ],
    "include-media": [
      "dist/*.scss"
    ],
    "sassy-maps": [
      "sass/**/*.scss"
    ],
    "modernizr": [
      "modernizr.js"
    ],
    "nwmatcher": [
      "src/**/*.js"
    ],
    "selectivizr": [
      "selectivizr.js"
    ],
    "respond": [
      "dest/*.js"
    ]
  }
}
