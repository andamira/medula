{
  "name": "Medula",
  "devDependencies": {
    "normalize.css": "*",
    "susy": "*",
    "include-media": "*",
    "sassy-maps": "*",
    "modernizr": "*",
    "bootstrap-sass-official": "*",
    "animate-sass": "*"
  },
  "ignore": [ "" ],
  "preen": {
    "normalize.css": [
      "*.css"
    ],
    "susy": [
      "sass/**/*.scss"
    ],
    "include-media": [
      "dist/*.scss"
    ],
    "sassy-maps": [
      "sass/**/*.scss"
    ],
    "modernizr": [
      "modernizr.js"
    ]
  }
}
